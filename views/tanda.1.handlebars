  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <h2>Create Tanda!</h2>
        <div class="form-group">
          <label for="exampleFormControlInput1">Nombre de Tanda</label>
          <input style="background-color:white;" type="text" class="form-control" id="nombreTanda" placeholder="Nombre Tanda">
        </div>
        <div class="form-group">
          <label for="exampleFormControlSelect1">Numero de Participantes</label>
          <select style="background-color:white;" id="participantes">
            
        
            <option>4</option>
          
          </select>
        </div>
        <div class="form-group">
          <label for="exampleFormControlSelect1">Monto a dar por per√≠odo de Tiempos</label>
          <select style="background-color:white;" id="monto">
            <option>100</option>
            <option>200</option>
            <option>300</option>
            <option>500</option>
            <option>1000</option>
          </select>
        </div>

        <button id="sendTanda" class="btn btn-primary float-right">Submit</button>


      </div>
    </div>
  </div>
  <div class="container">
    <div class="row">
       {{#each examples}}
      <div class="col-8 offset-2 card">
        <div data-id="{{this.id}}" class="card-body">
          <table class="table">
  <thead>
    <tr>
      <th scope="col">Creada Por</th>
      <th scope="col">Nombre Tanda</th>
      <th scope="col">Capacidad</th>
      <th scope="col">Inscritos</th>
      <th scope="col">Monto de Tanda</th>
      <th scope="col">Status</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row">1</th>
      <td>{{this.organizadorNombre}}</td>
      <td>{{this.nombre}}</td>
      <td>{{this.capacidad}}</td>
      <td>{{this.inscritos}}</td>
      <td>{{this.monto}}</td>
      <td>{{this.activa}}</td>
    </tr>
    <tr>
      <th scope="row"></th>
      <td></td>
      
      <td><button style="align:center;"  id="{{this.id}}" class="btn btn-primary unirte"> Unirte </button></td>
      <td userid={{this.id}}><button style="align:center;"  id="{{this.id}}" class="btn btn-primary detalle"> Detalle </button></td>
      <td></td>
    </tr>
    
  </tbody>
  
</table>
